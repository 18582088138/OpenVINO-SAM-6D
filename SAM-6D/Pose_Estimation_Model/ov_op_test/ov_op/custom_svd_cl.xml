<CustomLayer name="CustomSVD" type="SimpleGPU" version="1">
    <Kernel entry="ov_custom_svd">
        <Source filename="custom_svd.cl"/>
    </Kernel>
    <Buffers>
        <Tensor arg-index="0" type="input" port-index="0" format="BFYX"/>   <!-- input (B, M, N) --> 
        <Tensor arg-index="1" type="output" port-index="0"/> <!-- U (B, M, M) --> 
        <Tensor arg-index="2" type="output" port-index="1"/> <!-- S (B, min(M,N)) --> 
        <Tensor arg-index="3" type="output" port-index="2" /> <!-- V (B, N, N) --> 
    </Buffers>
    <!-- <CompilerOptions options="-cl-mad-enable -cl-fast-relaxed-math -cl-no-signed-zeros"/> -->
    <CompilerOptions options="-cl-mad-enable"/>
    <WorkSizes global="B,1,1"/>
</CustomLayer>
